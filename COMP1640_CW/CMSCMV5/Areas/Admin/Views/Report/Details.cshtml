@model CMSCMV5.DAO.Report

@if (Model != null)
{
    <div style="margin: 0 auto;width: 100%;">
        <table border="1" cellpadding="0" cellspacing="0" style="width: 100%">
            <tbody>
                <tr>
                    <td colspan="2" style="width:568px;">
                        <p align="center"><strong>COURSE MONITORING REPORT</strong></p>
                    </td>
                </tr>
                <tr>
                    <td style="width:130px;">
                        <p style="margin-left:5px">Academic Session</p>
                    </td>
                    <td style="width:438px;">
                        <p>&nbsp;</p>
                    </td>
                </tr>
                <tr>
                    <td style="width:130px;">
                        <p style="margin-left:5px">Course Code + title:</p>
                    </td>
                    <td style="width:438px;">
                        <p>&nbsp;</p>
                    </td>
                </tr>
                <tr>
                    <td style="width:130px;">
                        <p style="margin-left:5px">Course Leader:</p>
                    </td>
                    <td style="width:438px;">
                        <p style="margin-left:5px">@Model.CLID</p>
                    </td>
                </tr>
                <tr>
                    <td style="width:130px;">
                        <p style="margin-left:5px">Student count</p>
                    </td>
                    <td style="width:438px;">
                        <p style="margin-left:5px">@Model.Studentcount</p>
                    </td>
                </tr>
            </tbody>
        </table>

        <p align="center">&nbsp;</p>

        @Html.Raw(HttpUtility.UrlDecode(Model.StatisticalData))

        <p align="center">&nbsp;</p>

        <p align="center">&nbsp;</p>

        @Html.Raw(HttpUtility.UrlDecode(Model.GradeDistributionData))

        <p align="center">&nbsp;</p>


        <table border="1" cellpadding="0" cellspacing="0" style="width: 100%">
            <tbody>
                <tr>
                    <td style="width:568px;">
                        <p><strong>General comments</strong></p>

                        <p>When you complete this section, at a minimum, you should address the following:</p>

                        <p>&nbsp;</p>

                        <ol>
                            <li>The overview of the Course Leader (to include; comments on available statistics, the range of marks, assessment diet and any issues affecting the delivery of the course this year).</li>
                            <li>Student Evaluation and Feedback.</li>
                            <li>Comments of the External Examiner.</li>
                            <li>A review of the previous year&rsquo;s action plan.</li>
                        </ol>
                    </td>
                </tr>
                <tr>
                    <td style="width:568px;">
                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>
                    </td>
                </tr>
            </tbody>
        </table>

        <p>&nbsp;</p>

        <table border="1" cellpadding="0" cellspacing="0" style="width: 100%">
            <tbody>
                <tr>
                    <td style="width: 568px;">
                        <p>
                            <strong>Action to be taken</strong>
                        </p>
                    </td>
                </tr>
                <tr>
                    <td style="width: 568px;">
                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>

                        <p>&nbsp;</p>
                    </td>
                </tr>
            </tbody>
        </table>
        @*<div class="row">
                <div class="col-md-12" style="margin-top: 20px">
                    <button type="submit" onclick="return confirm('Are you sure you want to submit!');" class="btn btn-success pull-right">Submit report</button>
                </div>
            </div>*@
    </div>
}